<div class="mt-4">
    <h3>My course</h3>
</div>
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Time created</th>
            <th scope="col">VideoId</th>
        </tr>
    </thead>
    <tbody>
        {{#each courses}}
        <tr>
            <th scope="row">{{sum @index 1}}</th>
            <td>{{this.name}}</td>
            <td>{{this.createdAt}}</td>
            <td>{{this.videoId}}</td>
            <td>
                <a href="/courses/{{this._id}}/edit" class="badge badge-primary">Change</a>
                <a href="#" class="badge badge-primary">Delete</a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>